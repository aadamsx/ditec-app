{
  "name": "@polymer/shop",
  "version": "0.8.0",
  "contributors": [
    "The Polymer Authors",
    "McInnis Studio"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.19",
    "@polymer/iron-a11y-keys-behavior": "^3.0.0-pre.26",
    "@polymer/iron-behaviors": "^3.0.0-pre.26",
    "@polymer/iron-dropdown": "^3.0.0-pre.26",
    "@polymer/iron-fit-behavior": "^3.0.0-pre.26",
    "@polymer/iron-icon": "^3.0.0-pre.19",
    "@polymer/iron-icons": "^3.0.0",
    "@polymer/iron-image": "^3.0.0-pre.19",
    "@polymer/iron-overlay-behavior": "^3.0.0-pre.19",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/lit-element": "^0.5.0",
    "@polymer/neon-animation": "^3.0.0-pre.26",
    "@polymer/paper-dropdown-menu": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.0-pre.19",
    "@polymer/paper-item": "^3.0.0-pre.25",
    "@polymer/paper-listbox": "^3.0.0-pre.25",
    "@polymer/paper-menu-button": "^3.0.0-pre.25",
    "@polymer/paper-spinner": "^3.0.0-pre.19",
    "@polymer/paper-styles": "^3.0.0-pre.26",
    "@polymer/paper-tabs": "^3.0.1",
    "@polymer/polymer": "3.0.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "lit-html": "^0.10.0",
    "pwa-helpers": "^0.8.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.8.0",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.1.0",
    "puppeteer": "^1.4.0",
    "renamer": "^0.6.1",
    "replace": "^1.0.0",
    "wct-browser-legacy": "^1.0.1"
  }
}
